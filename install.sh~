echo Installing Py4A...
adb install PythonForAndroid_r5.apk
echo Installing sl4a...
adb install sl4a_r3.apk
echo Installing Ros Essentials...
adb push ~/ROS_ANDROID/rospy_android/com.googlecode.pythonforandroid /sdcard/com.googlecode.pythonforandroid
echo Copying example scripts...
adb push ~/ROS_ANDROID/rospy_android/sl4a /sdcard/sl4a
echo Installation Complete
